# Obsidian Kanban Dashboard Configuration
# Copy this file to .env and update the values for your setup

# Path to your Obsidian vault's daily notes folder
# Examples:
#   macOS:   /Users/yourname/Documents/My Vault/Daily Notes
#   Linux:   /home/yourname/obsidian-vault/Daily Notes
#   Windows: C:/Users/yourname/Documents/My Vault/Daily Notes
VAULT_PATH=/path/to/your/vault/Daily Notes

# Server port for the API backend
API_PORT=3001

# React app port (used by react-scripts)
PORT=3000
